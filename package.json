		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"knip": "^3.8.4",
		"prettier": "^3.1.1",
		"rollup-plugin-visualizer": "5.11.0",
		"turbo": "1.11.2",
		"tsx": "4.7.0",
		"typescript": "^5.3.3",
		"vite": "^5.0.10",
		"vite-tsconfig-paths": "4.2.2",
		"vitest": "^1.1.0"
