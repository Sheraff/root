{
	"name": "repo",
	"version": "0.0.0-alpha.3",
	"license": "MIT",
	"private": true,
	"scripts": {
		"analyze": "turbo analyze --filter client",
		"dev": "FORCE_COLOR=1 turbo dev",
		"test": "FORCE_COLOR=1 turbo root:test",
		"test:ci": "FORCE_COLOR=1 turbo test",
		"build": "FORCE_COLOR=1 turbo build",
		"serve": "NODE_ENV=production node --env-file=.env dist/server/app.mjs",
		"lint": "FORCE_COLOR=1 turbo lint root:lint --continue --log-order=grouped",
		"lint:fix": "FORCE_COLOR=1 turbo lint:fix root:lint:fix --continue --log-order=grouped",
		"format": "FORCE_COLOR=1 turbo format root:format --continue --log-order=grouped",
		"format:fix": "FORCE_COLOR=1 turbo format:fix root:format:fix --continue --log-order=grouped",
		"tsc": "FORCE_COLOR=1 turbo tsc root:tsc --continue --log-order=grouped",
		"knip": "knip",
		"spell": "FORCE_COLOR=1 turbo spell root:spell --continue --log-order=grouped",
		"clear": "FORCE_COLOR=1 turbo clear root:clear --continue",
		"root:test": "vitest",
		"root:tsc": "echo \"done\"",
		"root:tsc:app": "tsc --noEmit --project tsconfig.app.json",
		"root:tsc:tools": "tsc --noEmit --project tsconfig.tools.json",
		"root:lint": "eslint \"*.ts\" \"types/*\"",
		"root:lint:fix": "eslint \"*.ts\" \"types/*\" --fix",
		"root:format": "prettier --check \"*.ts\" \"*.json\" \"types/*\" \".github/**/*\" --config .prettierrc.json --log-level warn",
		"root:format:fix": "prettier --write \"*.ts\" \"*.json\" \"types/*\" \".github/**/*\" --config .prettierrc.json --log-level warn",
		"root:spell": "cspell --config .cspell.json --quiet --gitignore \"*\" \"types/*\"",
		"root:clear": "rm -rf dist; rm -rf node_modules; rm -rf .turbo; find . -name '*.tsbuildinfo' -type f -delete;"
	},
	"dependencies": {
		"@fastify/cookie": "^9.3.1",
		"@fastify/session": "^10.7.0",
		"@fastify/static": "7.0.1",
		"@tanstack/react-query": "^5.18.1",
		"@tanstack/react-query-devtools": "^5.18.1",
		"@vlcn.io/crsqlite": "0.16.3",
		"@vlcn.io/react": "3.1.0",
		"@vlcn.io/ws-common": "0.2.0",
		"better-sqlite3": "^9.4.0",
		"clsx": "^2.1.0",
		"dotenv": "^16.4.1",
		"fastify": "^4.26.0",
		"grant": "^5.4.22",
		"pino-pretty": "10.3.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"valibot": "0.28.1"
	},
	"devDependencies": {
		"@fastify/http-proxy": "^9.4.0",
		"@total-typescript/ts-reset": "0.5.1",
		"@types/better-sqlite3": "^7.6.9",
		"@types/node": "20.11.17",
		"@types/react": "18.2.55",
		"@types/react-dom": "18.2.19",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"@vitejs/plugin-react-swc": "3.6.0",
		"@vitest/web-worker": "^1.2.2",
		"chalk": "^5.3.0",
		"chokidar": "3.6.0",
		"cspell": "^8.3.2",
		"esbuild": "0.20.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"knip": "^4.5.0",
		"prettier": "^3.2.5",
		"rollup-plugin-visualizer": "5.12.0",
		"tsx": "4.7.0",
		"turbo": "1.12.3",
		"typed-css-modules": "^0.9.1",
		"typescript": "^5.3.3",
		"typescript-plugin-css-modules": "^5.0.2",
		"vite": "5.0.12",
		"vitest": "^1.2.2",
		"vitest-github-actions-reporter": "^0.11.1"
	},
	"engines": {
		"node": ">=20.9.0"
	}
}
