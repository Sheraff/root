{
	"name": "fullstack-typescript-project",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"analyze": "turbo analyze --filter client",
		"dev": "pnpm build:common && FORCE_COLOR=1 turbo dev",
		"test": "vitest",
		"build": "FORCE_COLOR=1 turbo build",
		"serve": "NODE_ENV=production node --env-file=.env dist/server/app.mjs",
		"lint": "FORCE_COLOR=1 turbo lint",
		"lint:fix": "FORCE_COLOR=1 turbo lint:fix",
		"format": "FORCE_COLOR=1 turbo format",
		"format:fix": "FORCE_COLOR=1 turbo format:fix",
		"tsc": "FORCE_COLOR=1 turbo tsc",
		"deps": "pnpm build:common && knip",
		"clear": "FORCE_COLOR=1 turbo clear; rm -rf dist; rm -rf node_modules; rm -rf .turbo;",
		"build:common": "FORCE_COLOR=1 turbo tsc --log-order=grouped --filter=scripts --filter=shared",
		"postinstall": "pnpm build:common"
	},
	"dependencies": {
		"@fastify/cookie": "^9.2.0",
		"@fastify/session": "^10.7.0",
		"@fastify/static": "6.12.0",
		"@tanstack/react-query": "^5.14.6",
		"@tanstack/react-query-devtools": "^5.14.6",
		"@vlcn.io/crsqlite": "0.16.1",
		"@vlcn.io/react": "3.1.0",
		"@vlcn.io/ws-common": "0.2.0",
		"better-sqlite3": "^9.2.2",
		"dotenv": "^16.3.1",
		"fastify": "^4.25.1",
		"grant": "^5.4.22",
		"pino-pretty": "10.3.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"valibot": "0.24.1"
	},
	"devDependencies": {
		"@total-typescript/ts-reset": "0.5.1",
		"@types/better-sqlite3": "^7.6.8",
		"@types/node": "20.10.5",
		"@types/react": "18.2.45",
		"@types/react-dom": "18.2.18",
		"@typescript-eslint/eslint-plugin": "^6.15.0",
		"@typescript-eslint/parser": "^6.15.0",
		"@vitejs/plugin-react-swc": "3.5.0",
		"chokidar": "3.5.3",
		"esbuild": "0.19.10",
