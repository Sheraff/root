{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "dependsOn": [],
      "cache": false
    },
    "shared#build": {
      "outputs": [
        "shared/dist/**"
      ]
    },
    "build": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "shared#build"
      ]
    },
    "test": {
      "dependsOn": [],
      "outputs": [
        ".vitest/**"
      ]
    },
    "serve": {
      "dependsOn": [
        "build"
      ],
      "persistent": true
    }
  }
}