{
	"$schema": "https://unpkg.com/knip@4/schema.json",
	"ignoreDependencies": [
		// used by fastify, with just a string reference, no import
		"pino-pretty",
		// might not be used everywhere yet
		"assets"
	],
	"ignore": [
		// avoid test files being the only import of something and that something not being marked as unused
		"**/*.test.ts",
		// knip doesn't seem to understand that these are used by just being included in a tsconfig file
		"**/types/*.d.ts"
	],
	"includeEntryExports": true
}
