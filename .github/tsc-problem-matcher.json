{
	"problemMatcher": [
		{
			"owner": "tsc-foo",
			"pattern": [
				{
					"regexp": "([a-z:\\/]+)\\s([^\\s].*)[\\(:](\\d+)[,:](\\d+)(?:\\):\\s+|\\s+-\\s+)(error|warning|info)\\s+TS(\\d+)\\s*:\\s*(.*)$",
					"file": 2,
					"line": 3,
					"column": 4,
					"severity": 5,
					"code": 6,
					"message": 7
				}
			]
		}
	]
}
