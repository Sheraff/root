{
	"name": "assets",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"tsc:build": "tsx ./esbuild.ts --build",
		"tsc:watch": "tsx ./esbuild.ts",
		"dev": "pnpm tsc:watch",
