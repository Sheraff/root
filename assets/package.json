{
	"name": "assets",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"tsc:build": "tsx ./esbuild.ts --build",
		"tsc:lib": "tsc --noEmit --project tsconfig.lib.json",
		"tsc:tools": "tsc --noEmit --project tsconfig.tools.json",
		"tsc": "echo 'done'",
		"dev": "tsx ./esbuild.ts",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"spell": "cspell --config ../.cspell.json --quiet --gitignore '**/*'",
		"clear": "rm -rf node_modules; rm -rf .turbo; find src -name '*.d.ts*' -type f -delete"
	},
	"exports": {
		"./*": "./src/*"
	},
	"devDependencies": {
		"script": "workspace:script@*"
	}
}
