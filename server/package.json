{
	"name": "server",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "node ./esbuild.mjs",
		"test": "vitest",
		"build": "node ./esbuild.mjs --build",
		"ts-check": "tsc --noEmit",
		"lint": "eslint . --config ../.eslintrc.json",
		"lint:fix": "eslint . --config ../.eslintrc.json --fix",
		"format": "prettier --check . --config ../.prettierrc --log-level warn",
		"format:fix": "prettier --write . --config ../.prettierrc --log-level warn"
	},
	"dependencies": {
		"@fastify/cookie": "^9.2.0",
		"@fastify/session": "^10.7.0",
		"@fastify/static": "6.12.0",
		"better-sqlite3": "^9.2.2",
		"dotenv": "^16.3.1",
		"fastify": "^4.24.3",
		"grant": "^5.4.22",
		"pino-pretty": "10.2.3",
		"valibot": "0.22.0"
	},
	"devDependencies": {
		"@types/better-sqlite3": "^7.6.8",
		"@types/node": "20.10.4",
		"esbuild": "0.19.8",
		"vite-tsconfig-paths": "4.2.2",
		"vitest": "^1.0.2"
	},
	"engines": {
		"node": ">=20.9.0"
	}
}
