{
	"$schema": "https://turbo.build/schema.json",
	"extends": ["//"],
	"pipeline": {
		"build": {
			"outputs": ["../dist/sw/*"],
			"inputs": [
				"*",
				"**/*",
				"../shared/src/**",
				"../scripts/src/**",
				"../dist/client/index.html",
				"../dist/client/assets/**"
			],
			"dependsOn": ["client#build", "scripts#tsc"]
		},
		"tsc": {
			"dependsOn": ["scripts#tsc"],
			"inputs": ["*", "**/*", "../shared/src/**", "../scripts/.dist/**", "../types/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true,
			"inputs": ["*", "**/*", "../shared/src/**", "../scripts/.dist/**", "../types/**"],
			"outputs": ["../client/public/sw.js"],
			"dependsOn": ["scripts#tsc"]
		}
	}
}
